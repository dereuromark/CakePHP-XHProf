{
	"name": "renan/cakephp-xhprof",
	"type": "cakephp-plugin",
	"description": "Plugin that quickly enables XHProf profiling for your CakePHP application.",
	"keywords": ["CakePHP", "XHProf", "debug", "profiling"],
	"homepage": "http://github.com/renan/CakePHP-XHProf",
	"license": "MIT",
	"authors": [
		{
			"name": "Renan GonÃ§alves",
			"email": "renan.saddam@gmail.com"
		},
		{
			"name": "Mark Scherer",
			"email": "euromark@web.de"
		}
	],
	"require": {
		"php": ">=5.4",
		"cakephp/cakephp": "~3.1",
		"ext-xhprof": "*"
	},
	"autoload": {
		"psr-4": {
			"XHProf\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"XHProf\\Test\\": "tests",
			"Cake\\Test\\": "vendor/cakephp/cakephp/tests",
			"TestApp\\": "tests/TestApp"
		}
	}
}
